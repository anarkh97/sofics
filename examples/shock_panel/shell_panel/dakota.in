environment
  tabular_data

method
  output debug 
  soga
    fitness_type 
    merit_function
      constraint_penalty = 10.0
    replacement_type 
      elitist
    convergence_type 
      best_fitness_tracker
        percent_change = 0.0 
        num_generations = 5
    max_iterations = 10
    max_function_evaluations = 2500
    scaling
    population_size = 20
    print_each_pop
    initialization_type
      unique_random
    crossover_type
      shuffle_random
        num_parents = 6
        num_offspring = 4
        crossover_rate = 0.8
    mutation_type
      replace_uniform
      mutation_rate = 0.25
    seed = 1649919949

model
  single

variables
  active design
  continuous_design = 3
    initial_point      1.0       1.0       1.0
    lower_bounds       0.4       0.4       0.4
    upper_bounds       4.0       4.0       4.0

interface
  analysis_driver = '../../../build/src/driver.sh --config config.sh --pre pre_pro.sh --post post_pro.sh'
  failure_capture
    recover 100000
  fork
    asynchronous
    evaluation_concurrency = 4 
    local_evaluation_scheduling = static
    parameters_file = 'params.in'
    results_file    = 'results.out'
    file_tag
    file_save
    work_directory 
      named = 'evaluation'
      directory_tag 
      directory_save
	   
responses
  objective_functions = 1
  no_gradients
  no_hessians
